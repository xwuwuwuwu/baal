{
    "version": "2.0",
    "extensions": {
        "http": {
            "routePrefix": "",
            "maxOutstandingRequests": 20,
            "maxConcurrentRequests": 10,
            "dynamicThrottlesEnabled": false
        }
    },
    "logging": {
        "fileLoggingMode": "never",
        "logLevel": {
            "default": "Error",
            "Host.Results": "Error",
            "Host.Aggregator": "Error",
            "Function": "Error",
            "Function.UrlGenerator.User": "Warning",
            "Function.download.User": "Warning"
        }
    }
}
